globalprocedure ProjScopeSClassArrayOnOpen(var Array string astr,var Integer acnt)BEGIN  record ProjScopeBlock PSB;  row ProjScopeBlock PSBrw;  Integer i,rwcnt;  LogText(0,"ProjScaleBlock");  BlockLoad(PSB);  rwcnt = MatRowCnt(PSB);  for (i=0;i<rwcnt;i=i+1) begin    MatRowGet(PSB,i,PSBrw);    LogText(0,PSBrw.Code);    if (NonBlank(PSBrw.Code)) then begin    	astr[acnt] = Left(PSBrw.Code & "    ",14);    	astr[acnt] = astr[acnt] & PSBrw.Name;    	acnt = acnt + 1;    end;  end;  RETURN;END;globalfunction Boolean ProjScopeSClassOnOpenWindow(Integer wn)begin  Array string 255 astr;  Integer i,acnt;  ProjScopeSClassArrayOnOpen(astr,acnt);  for (i=0;i<acnt;i=i+1) begin    SetListString(wn,len(astr[i]),astr[i],false);      end;  ProjScopeSClassOnOpenWindow = false;  return;end;