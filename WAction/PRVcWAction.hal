function Boolean PRDClassTerminatedButtonAfter(Integer wn,Boolean changedf)
begin        
  record PRVc PRr;  
  record PRVc PR2r;
  Boolean res;
  Integer normalmode,updatemode;
  Date bd;

  DeselectWindow(wn,false);
  normalmode = 0;//Rs_normal
