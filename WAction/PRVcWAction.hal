global updating
procedure ClonePRDsm()		//Edit by Victor 12.12.14
BEGIN
	Integer wn;
  record PRVc PRr;
  wn = CurWindow;
  if (WindowState(wn)==Rs_normal) then begin
    GetWindowRecord(wn,PRr);
    if(nonblank(PRr.MotherCode))then begin
	    ClonePRDExecute(PRr);
	  end else begin
	  	Messagebox(0,"Нельзя клонировать старший проект!");
	  end;
  end else begin
    Beep;
  end;
  RETURN;
END;